                                    <div class="margin-10px-top font-size14">@Model.Doctors.Where(e => e.WorkTime.WorkTimeWeekDayRelation.Select(k => k.WeekDay).Where(k => k.ToString() == "Wednesday").FirstOrDefault() != 0 && e.WorkTime.StartedTime.Hour <= @i && e.WorkTime.EndedTime.Hour >= @i + 1).Select(e => e.Name).FirstOrDefault()</div>
